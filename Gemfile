# A sample Gemfile
source "https://rubygems.org"

# Get data from and to Podio
gem 'podio'

# Spreadsheet Handling
gem 'roo', '~> 2.4.0'

# Make terminal results pretty
gem 'colorize', '~> 0.8.1'

# Shim to load environment variables from .env into ENV in development.
gem 'dotenv'

# Troubleshooting
gem 'pry', '~> 0.10.3'
gem 'rb-readline'
